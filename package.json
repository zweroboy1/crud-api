{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "Node.JS CRUD API - RSS Assignment",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:dev": "tsnd -r tsconfig-paths/register ./src/index.ts",
    "start:multi": "cross-env MULTIMODE=true tsnd -r tsconfig-paths/register ./src/index.ts",
    "start:prod": "npm run build && node ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zweroboy1/crud-api.git"
  },
  "dependencies": {
    "dotenv": "^16.4.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "nodejs",
    "crud",
    "api"
  ],
  "author": "Oleksii Roman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zweroboy1/crud-api/issues"
  },
  "homepage": "https://github.com/zweroboy1/crud-api#readme"
}